--
-- Name: pending_certifications_by_bank; Type: VIEW; Schema: public; Owner: agro_user
--

CREATE VIEW public.pending_certifications_by_bank AS
 SELECT bank_name,
    count(*) AS pending_count,
    sum(approved_amount) AS total_requested_amount,
    min(created_at) AS oldest_request,
    max(created_at) AS newest_request
   FROM public.certifications c
  WHERE ((status)::text = 'pendiente'::text)
  GROUP BY bank_name;


ALTER VIEW public.pending_certifications_by_bank OWNER TO agro_user;

